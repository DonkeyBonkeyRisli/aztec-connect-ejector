{
  "name": "Aztec V2 ejector",
  "forwardPorts": [8081],
  "hostRequirements": {
    "cpus": 16,
    "memory": "16gb",
    "storage": "32gb"
  },
  "customizations": {
    "codespaces": {
      "openFiles": ["manifest.json"]
    },
    "vscode": {
      "extensions": [],
      "settings": {
        "github.codespaces.defaultIdleTimeout": 45 // 45 mins
      }
    }
  }
}
